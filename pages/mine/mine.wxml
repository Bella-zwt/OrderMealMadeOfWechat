<view class="container">
	<view class="header">
		<view class="head">
			<image src="{{userHead}}"></image>
		</view>
		<view class="username">
		<text>{{userName}}</text>
		</view>
	</view>
	<view class="body-wrap">
		<view class="item" bindtap="toMycollection">
			<view class="icon">
				<image src="/images/收藏.png"></image>
			</view>
			<view class="item-text">
				<text>我的收藏</text>
			</view>
			<view class="enter">
			<mp-icon icon="arrow" color="black" size="{{15}}"></mp-icon>
			</view>
		</view>
		<view class="item" bindtap="toHistory">
			<view class="icon">
				<image src="/images/文档.png"></image>
			</view>
			<view class="item-text">
				<text>历史订单</text>
			</view>
			<view class="enter">
			<mp-icon icon="arrow" color="black" size="{{15}}"></mp-icon>
			</view>
		</view>
		<view class="item" bindtap="toJudge">
			<view class="icon">
				<image src="/images/对话信息.png"></image>
			</view>
			<view class="item-text">
				<text>我的评价</text>
			</view>
			<view class="enter">
			<mp-icon icon="arrow" color="black" size="{{15}}"></mp-icon>
			</view>
		</view>
		<view class="item" bindtap="toMember">
			<view class="icon">
				<image src="/images/通讯录.png"></image>
			</view>
			<view class="item-text">
				<text>会员信息</text>
			</view>
			<view class="enter">
			<mp-icon icon="arrow" color="black" size="{{15}}"></mp-icon>
			</view>
		</view>
		<view class="item" bindtap="toStore">
			<view class="icon">
				<image src="/images/商家.png"></image>
			</view>
			<view class="item-text">
				<text>商家信息</text>
			</view>
			<view class="enter">
			<mp-icon icon="arrow" color="black" size="{{15}}"></mp-icon>
			</view>
		</view>
	</view>
</view>